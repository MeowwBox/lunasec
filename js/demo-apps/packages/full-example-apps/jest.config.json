"jest": {
    "roots": [
        "<rootDir>/src"
    ],
        "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "snapshotSerializers": [
        "enzyme-to-json/serializer"
    ],
        "setupFilesAfterEnv": [
        "<rootDir>/src/client/setupEnzyme.ts"
    ],
        "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
        "globals": {
        "ts-jest": {
            "tsconfig": "<rootDir>/src/client/tsconfig-for-tests.json"
        }
    }
},